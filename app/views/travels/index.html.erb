<legend>My Travels</legend>

<ul class="thumbnails">  
			<%
				@currentFriend = Friend.where(user_name: @@userName).first
				@myTravels = @currentFriend.travels.all
				@myTravels.each do |travel_i| %>

        
          <div class="row-fluid span12">
	   <li>
	    <div class="span8">
<!-- :style => {:medium => "120x120!"} -->
            	<%=image_tag(travel_i.photo.url, id:travel_i.id, alt: "Icon", :height => "20%", :width => "20%" )%>
		<% editButtonId = "myTravelEditButton:"+travel_i.id.to_s %>
                <div><a href="#" id=<%=editButtonId%>>Edit</a> <a href="#">Delete</a></div>  
	    </div>
	    <div class="row-fluid span4">    
		
            </div> 
	   </li>
	  </div>    
	<br>
			<%end%>

      </ul>  




<a href="#" id="newTravelId">New Travel</a>
